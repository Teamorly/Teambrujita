<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <title>Redirección Secuencial</title>
  <script>
    const grupos = [
      "https://chat.whatsapp.com/HsaBuLsrdPO4V21yjIQv47", // Grupo 1
      "https://chat.whatsapp.com/LgnVlowLUYT9cZae0bEI5V", // Grupo 2
      "https://chat.whatsapp.com/D4qSliHepxsEVmCFHm7fZK", // Grupo 3
      "https://chat.whatsapp.com/KFm5iHFYDgdA5RJ7LXWddZ", // Grupo 4
      "https://chat.whatsapp.com/Db2qc5V6ramIhAMh3eLcIC"  // Grupo 5
    ];

    // Al cargar la página, redirige secuencialmente y guarda la posición
    window.onload = function () {
      let index = parseInt(localStorage.getItem("grupoIndex")) || 0;
      window.location.href = grupos[index];
      index = (index + 1) % grupos.length;
      localStorage.setItem("grupoIndex", index);
    };
  </script>
</head>
<body>
  <p>Redirigiendo...</p>
</body>
</html>
